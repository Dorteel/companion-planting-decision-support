                    <rdf:Description rdf:about="http://www.semanticweb.org/kai/ontologies/2024/companion-planting#Flora"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/kai/ontologies/2024/companion-planting#incompatibleNeighbour"/>
                        <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kai/ontologies/2024/companion-planting#Flora"/>
    </owl:Class>
    


